<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.lucasfrancisco.modulopatrimonio">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_01"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ListaObjetoActivity"></activity>
        <activity android:name=".activities.TesteObjetoActivity"></activity>
        <activity android:name=".activities.ObjetoActivity"></activity>

        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.NovoObjetoActivity" />
        <activity
            android:name=".activities.NovoPatrimonioActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.NovaEmpresaActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.NovoEnderecoActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.EnderecoActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.NovoSetorActivity"
            android:parentActivityName=".activities.MainActivity" />
    </application>

</manifest>